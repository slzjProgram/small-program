/**index.wxss**/
@import '../template/goods-item/goods-item-template.wxss';
@import '../template/goods-item/goods-item-two-template.wxss';
@import '../template/category/category-two-template.wxss';
@import '../template/search/search-template.wxss';

.swiper-image {
  width: 100%;
}

.nav-item {
  width: 25%;
  display: flex;
  align-items: center;
  flex-direction: column;
  padding: 20rpx;
}

.nav-img {
  width: 110rpx;
  height: 114rpx;
}
.nav-text{
  padding-top: 20rpx;
  font-size: 30rpx;
}
.category-title{
  padding-top: 15rpx;
  padding-left: 20rpx;
  padding-bottom: 15rpx;
  display: flex;
  align-items: center;
}
.small-icon-img{
  width: 45rpx;
  height:45rpx;
  margin-right: 15rpx;
}
.small-text{
  font-size: 30rpx;
   font-weight: 900;
}

.cart-shop-list {
  margin-bottom: 200rpx;
}

.cart-shop-img {
  width: 200rpx;
  height: 200rpx;
  border-radius: 15rpx;
}

.group-buy{
  width: 300rpx;
  height: 300rpx;
}

.cart-shop-title {
  font-size: 24rpx;
  font-weight: bold;
}
.cart-shop-select-info{
  font-size: 24rpx;
  color: darkgray;
}
.cart-shop-price{
  font-size: 24rpx;
  color: red;
}


.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent
    url(data:image/svg+xml;base64,)
    no-repeat;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}


.block-grey{
  background-color: gainsboro;
}

.home-goods-item-two {
  width: 100%;
  display: box;
}

.home-container{
  background-color: #F8F7F7;
}

page {
  background-color: #fff;
}

.my_store {
  width: 100%;
  padding: 30rpx;
  box-sizing: border-box;
  background-color: #e41436;
  display: flex;
  flex-direction: row;
  justify-content: start;
  font-size: 24rpx;
  color: #fff;
}

.my_store .my-store_left image {
  width: 140rpx;
  height: 140rpx;
  border-radius: 10rpx;
  margin-right: 15rpx;
  border: 1px solid #fff;
}

.my-store_right .my-store_name {
  font-size: 32rpx;
  margin-bottom: 6rpx;
}

.my-store_right>text {
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.home-category {
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #8b8b8b;
  background-color: #fff;
}

.home-category-scroll .selected {
  color: #000;
}

.page-section-title {
  padding: 0;
}

.swiper-item {
  display: block;
  height: 650rpx;
}

.swiper-item image {
  display: block;
  width: 100%;
}

.page-section-title {
  margin-top: 60rpx;
  position: relative;
}

.home-goods-item-two {
  width: 100%;
  display: box;
}

.goods-special-area {
  width: 100%;
}

.goods-special-area .special-box {
  width: 50%;
  display: inline-block;
}

.goods-special-area image {
  display: block;
  width: 100%;
}

.icon-more {
  position: absolute;
  right: 10rpx;
  width: 41rpx;
  height: 41rpx;
  color: #000;
}

.tab-h {
  height: 88rpx;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 88rpx;
  white-space: nowrap;
  left: 0;
  z-index: 99;
}

.tab-item {
  width: 100rpx;
  text-align: center;
  display: inline-block;
  font-size: 30rpx;
}

.tab-item.active {
  color: #000;
  position: relative;
}

.tab-item.active:after {
  content: "";
  display: block;
  height: 8rpx;
  width: 100rpx;
  background: #000;
  position: absolute;
  bottom: 0;
  border-radius: 16rpx;
}

.tab-item-none {
  width: 100rpx;
}

.category-two {
  width: 100%;
  display: box;
}

.category-two-show {
  display: block;
}

.category-two-display {
  display: none;
}

.icon-more-box {
  position: absolute;
  display: flex;
  flex-direction: column;
  align-items: center;
  right: 0;
  z-index: 600;
  width: 100rpx;
  height: 88rpx;
  background-color: #fff;
  text-align: center;
}

.home-category .icon-more {
  width: 90rpx;
  height: 100%;
  color: #000;
  font-weight: bold;
  margin-left: 50rpx;
  text-align: center;
  font-size: 41rpx;
}

.category-more {
  position: fixed;
  top: 80rpx;
  width: 100%;
  z-index: 501;
  background-color: #fff;
  color: #8b8b8b;
}

.category-more-b {
  padding: 20rpx 0;
}

.category-more .category-more-box {
  width: 25%;
  display: inline-block;
  text-align: center;
  font-size: 30rpx;
}

.category-more-item {
  height: 50rpx;
  line-height: 50rpx;
  margin: 10rpx;
  border: 1rpx solid #ccc;
  border-radius: 15rpx;
}

.category-more-title {
  text-align: center;
  font-size: 32rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-bottom: 1px solid #ccc;
}

.mask {
  z-index: 200;
  filter: alpha(opacity=50);
  -moz-opacity: 0.5;
  -khtml-opacity: 0.5;
  opacity: 0.2;
  height: 900rpx;
  background-color: #000;
  position: fixed;
  width: 100%;
  top: 0;
  overflow: hidden;
}

.body-view {
  width: 100%;
  text-align: center;
  font-size: 24rpx;
  color: #666;
}

.nav-bar{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  margin: 20rpx 0;
  padding: 10rpx 0;
  background-color: #fff;
  border-top: 4rpx solid #E3E3E3;
  border-bottom: 4rpx solid #E3E3E3;
  font-size: 25rpx
}

.nav-bar-item{
  height: 100%;
}

.circle-pink{
  width: 120rpx;
  height: 120rpx;
  background-color: #EFABB4;
  border-radius: 50%;
}

.circle-purple{
  width: 120rpx;
  height: 120rpx;
  background-color: #EFABDA;
  border-radius: 50%;
}

.circle-green{
  width: 120rpx;
  height: 120rpx;
  background-color: #ABEFE5;
  border-radius: 50%;
}
.circle-blue{
  width: 120rpx;
  height: 120rpx;
  background-color: #ABBFEF;
  border-radius: 50%;
}

.nav-bar image{
  width: 80rpx;
  height: 80rpx;
}